{
    "name": "weirdo/laravel-weirdopanel",
    "license": "MIT",
    "description": "Un panel de administraci√≥n basado en livewire",
    "authors": [
        {
            "name": "Angel Hidalgo",
            "email": "aghabrego@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^10",
        "weirdo/laravel-easyblade" : "^0.8",
        "laravel/framework": "^10",
        "livewire/livewire": "^3.0",
        "weirdo/dynamic-acl": "^1.0.5",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "WeirdoPanel\\" : "src/"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "WeirdoPanelTest\\" : "tests/"
        }
    },

    "extra": {
        "laravel" : {
            "providers" : [
                "WeirdoPanel\\WeirdoPanelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
